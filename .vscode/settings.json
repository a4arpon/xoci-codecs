{
  "editor.defaultFormatter": "denoland.vscode-deno",
  "deno.enablePaths": [
    "./"
  ],
  "editor.inlayHints.enabled": "off",
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "cody.codeActions.enabled": true,
  "editor.codeActionsOnSave": {
    "source.removeUnusedImports": "always"
  }
}